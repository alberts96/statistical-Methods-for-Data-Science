crossMatrixSplit <- function(pred,obs){
crossMatrixSplit <- function(pred,obs){
x <- c()
for (i in c(1:length(pred))){
if (pred[i] != obs[i]){
x <- c(x,"unknown")
}
else{
if (obs[i]=='active'){x <- c(x,'active')}
else{x <- c(x,'failed')}
}}
return(x)}
